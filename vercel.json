{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "regions": ["iad1"],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=0, stale-while-revalidate"
        }
      ]
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "staging": true,
      "gh-pages": false
    }
  },
  "ignoreCommand": "bash -c 'git diff --quiet HEAD^ HEAD -- . \":(exclude)website\" \":(exclude).github/workflows/deploy-docs.yml\"'",
  "public": true
}
